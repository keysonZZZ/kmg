<? package kmgChart
func viewLine(in Line)string{
?>
<div id="<?= in.domId ?>" style="width: <?=in.Width?>;height: <?=in.Height?>;">
</div>
<script>
    (function () {
        var line = echarts.init(document.getElementById(<?= in.domId ?>));
        var option = <?= raw(in.GetOptionString()) ?>;
        line.setOption(option);
    })();
</script>
<? } ?>
